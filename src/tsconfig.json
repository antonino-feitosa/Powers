{
    "compilerOptions": {
        "lib": [
            "es2022"
        ],
        "module": "commonjs",
        "target": "es2022",
        "moduleResolution": "node",
        "rootDir": ".",
        "allowJs": true,
        "sourceMap": true,
        "preserveConstEnums": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
    },
    "exclude": [
        "node_modules"
      ]
}